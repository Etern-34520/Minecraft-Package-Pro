<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefWidth="200.0" styleClass="background" type="VBox" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="title" style="-fx-font-size: 16px;" text="Title">
         <font>
            <Font size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ProgressBar fx:id="progressBar" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="3.0" prefWidth="0.0" progress="0.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="2.0" right="2.0" />
         </VBox.margin></ProgressBar>
      <HBox>
         <children>
            <Label fx:id="tipLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" text="tip" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="10.0" top="3.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="pause" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#pauseOrResume" prefHeight="17.0" prefWidth="17.0" style="-fx-font-size: 6px;" HBox.hgrow="NEVER">
               <font>
                  <Font size="6.0" />
               </font>
               <HBox.margin>
                  <Insets right="1.0" top="3.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancel" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="17.0" prefWidth="17.0" style="-fx-font-size: 6px;" HBox.hgrow="NEVER">
               <font>
                  <Font size="6.0" />
               </font>
               <HBox.margin>
                  <Insets right="5.0" top="3.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="moreInfo" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" text="moreInfo" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="8.0" left="10.0" top="5.0" />
         </VBox.margin>
      </Label>
   </children>
   <effect>
      <DropShadow height="20.0" radius="9.5" width="20.0">
         <color>
            <Color opacity="0.5453172326087952" />
         </color></DropShadow>
   </effect>
</fx:root>
